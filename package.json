{
  "name": "record-deck",
  "version": "1.0.0",
  "description": "Smooth BLDC with a positional encoder, via a constant jerk model with input smoothing via a kalman filter, suitable for low to high (28k) rpm applications",
  "main": "index.js",
  "scripts": {
    "generate:docs": "rm -rf docs/ && jsdoc -R ./calibration/README.md -d docs ./calibration/network-multicapture-source.js ./calibration/network-multicapture-sync.js ./calibration/combine-multicapture-files.js",
    "install:ibm-double-pendulum": "bash ./datasets/download-double-pendulum.sh",
    "install:venv": "python3 -m venv env",
    "install:venv2": "virtualenv -p /usr/bin/python3 env",
    "install:venv-deps": "pip3 install -r  pip.freeze && pip3 install -e .",
    "test:venv": "python3 test-env.py",
    "simulate:bldc": "npm run test:venv && bokeh serve --show tracking/simulating-duty-motor.py",
    "simulate:double-pendulum": "npm run test:venv && python datasets/process-double-pendulum.py ${npm_config_dataset} > datasets/data/double-pendulum/data${npm_config_dataset}.csv && bokeh serve --show tracking/pendulum-state-estimator.py --args ${npm_config_dataset}",
    "combine:rotation-voltage-network-data": "npm run test:venv && python3 ./calibration/network-multicapture-combine.py ${npm_config_run_id}",
    "smooth:rotation-voltage-data": "npm run test:venv && python3 calibration/smooth-rotation-voltage-data.py ${npm_config_run_id}",
    "inspect:rotation-voltage-data": "npm run test:venv && bokeh serve --port 8033 --show calibration/tune-rotation-voltage-data-smoothing.py --args ${npm_config_run_id}",
    "detect:zero-crossing": "npm run test:venv && python3 calibration/zero-cross-spark.py ${npm_config_run_id}",
    "cluster:zero-crossing": "npm run test:venv && python3 calibration/zero-cross-cluster-data.py ${npm_config_run_id} ${npm_config_number_of_poles} zero_crossing_detections.channels.all.json kmedoids_clustered_zero_crossing_channel_detections.all.json",
    "analyse:zero-crossing-channel-clusters": "npm run test:venv && python3 calibration/analyse-zero-cross-cluster-data.py ${npm_config_run_id} kmedoids_clustered_zero_crossing_channel_detections.all.json kmedoids_clustered_zero_crossing_channel_detections.all.analysis.json",
    "inspect:zero-crossing": "npm run test:venv && python3 calibration/zero-cross-inspect.py ${npm_config_run_id}",
    "cluster-inliers:zero-crossing": "npm run test:venv && python3 calibration/zero-cross-cluster-data.py ${npm_config_run_id} ${npm_config_number_of_poles} zero_crossing_detections.channels.inliers.json kmedoids_clustered_zero_crossing_channel_detections.inliers.json",
    "analyse-inliers:zero-crossing-channel-clusters": "npm run test:venv && python3 calibration/analyse-zero-cross-cluster-data.py ${npm_config_run_id} kmedoids_clustered_zero_crossing_channel_detections.inliers.json kmedoids_clustered_zero_crossing_channel_detections.inliers.analysis.json",
    "inspect-inliers:zero-crossing": "npm run test:venv && python3 calibration/zero-cross-inspect-inliers.py ${npm_config_run_id}",
    "perform-all-analysis": "npm run combine:rotation-voltage-network-data --run_id=${npm_config_run_id} && npm run smooth:rotation-voltage-data --run_id=${npm_config_run_id} && npm run detect:zero-crossing --run_id=${npm_config_run_id} && npm run cluster:zero-crossing --run_id=${npm_config_run_id} --number_of_poles=14 && npm run analyse:zero-crossing-channel-clusters --run_id=${npm_config_run_id} && npm run inspect:zero-crossing --run_id=${npm_config_run_id} && npm run cluster-inliers:zero-crossing --run_id=${npm_config_run_id} --number_of_poles=14 && npm run analyse-inliers:zero-crossing-channel-clusters --run_id=${npm_config_run_id} && npm run inspect-inliers:zero-crossing --run_id=${npm_config_run_id}",
    "update:venv-deps": "npm run test:venv && pip3 freeze > pip.freeze",
    "network-serial:collect-sync": "node calibration/network-multicapture-sync.js 8132 ${npm_config_run_id}",
    "network-serial:collect-source": "node calibration/network-multicapture-source.js ${npm_config_device_id} /dev/ttyACM0 ${npm_config_sync_host} 8132"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jk89/record-deck.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jk89/record-deck/issues"
  },
  "homepage": "https://github.com/jk89/record-deck#readme",
  "config": {
    "dataset": "14",
    "spark": {
      "master": "spark://10.0.0.3:6060",
      "local-interface": "kelsey",
      "project_file_dependencies": [
        "calibration/kmedoids2.py",
        "calibration/metrics.py",
        "calibration/analyse.py"
      ]
    }

  },
  "dependencies": {
    "cross-var": "^1.1.0",
    "jsdoc": "^3.6.11",
    "promise-socket": "^7.0.0",
    "serialport": "^10.4.0"
  }
}
